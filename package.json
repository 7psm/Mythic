{
  "name": "mythic",
  "version": "1.1.0",
  "type": "module",
  "description": "API Mythic avec Express",
  "main": "API/server.js",
  "scripts": {
    "start": "node API/server.js",
    "dev": "nodemon API/server.js",
    "security-check": "node src/js/scripts/security-check.js",
    "transfuscator": "node src/js/Obfuscator/TransFuscator.mjs",
    "tailwind:dev": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/output.css --watch",
    "tailwind:build": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/output.css --minify"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "discord.js": "^14.25.1",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.10",
    "npm": "^11.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "nodemon": "^3.1.11",
    "npx-run": "^2.1.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
